{#
/**
 * @file
 * Theme override for a field.
 */
#}
{% for item in items %}
  {% set link = item.content['#url'] %}
  {% set text = item.content['#title'] %}
  {% set external = item.is_external %}
  {% include "@suomidigi/misc/link.twig" with {
    link_url: link,
    link_text: text,
    external: external
  } %}
{% endfor %}
